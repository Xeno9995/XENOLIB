local player = Players.LocalPlayer

-- âœï¸ ØºÙŠÙ‘Ø± Ø§Ù„Ø§Ø³Ù… Ø­Ø³Ø¨ Ø§Ø³Ù… Ø§Ù„Ø·Ø§ÙˆÙ„Ø© Ø§Ù„Ø­Ù‚ÙŠÙ‚ÙŠ Ø¨Ø§Ù„Ù„Ø¹Ø¨Ø© Ø¥Ù† Ù„Ø²Ù… Ø§Ù„Ø£Ù…Ø±
local itemName = "Rage Table"  -- Ø£Ùˆ "Rage Table" Ø­Ø³Ø¨ Ø§Ù„Ù…Ø§Ø¨

-- Ø¯Ø§Ù„Ø© Ù„Ù„Ø¹Ø«ÙˆØ± Ø¹Ù„Ù‰ RemoteEvent Ø£Ùˆ RemoteFunction Ù„Ù„Ø´Ø±Ø§Ø¡
local function findBuyRemote()
	for _, v in pairs(ReplicatedStorage:GetDescendants()) do
		if (v:IsA("RemoteEvent") or v:IsA("RemoteFunction")) and v.Name:lower():find("buy") then
			return v
		end
	end
	return nil
end

-- Ø´Ø±Ø§Ø¡ ØªÙ„Ù‚Ø§Ø¦ÙŠ
local function autoBuyAngerTable()
	local remote = findBuyRemote()
	if remote then
		warn("ğŸª‘ ÙŠØªÙ… Ø´Ø±Ø§Ø¡ Ø·Ø§ÙˆÙ„Ø© Ø§Ù„ØºØ¶Ø¨...")

		if remote:IsA("RemoteEvent") then
			pcall(function()
				remote:FireServer(itemName)
			end)
		elseif remote:IsA("RemoteFunction") then
			pcall(function()
				remote:InvokeServer(itemName)
			end)
		end
	else
		warn("âŒ Ù„Ù… ÙŠØªÙ… Ø§Ù„Ø¹Ø«ÙˆØ± Ø¹Ù„Ù‰ RemoteEvent Ù„Ø´Ø±Ø§Ø¡ Ø·Ø§ÙˆÙ„Ø© Ø§Ù„ØºØ¶Ø¨.")
	end
end

