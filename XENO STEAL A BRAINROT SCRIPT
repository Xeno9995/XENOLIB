local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")

-- Ã˜Â­Ã™â€¦Ã˜Â§Ã™Å Ã˜Â© Ã˜Â¶Ã˜Â¯ Ã˜Â§Ã™â€Ã˜Â·Ã˜Â±Ã˜Â¯ Ã˜Â£Ã™Ë† Ã˜Â§Ã™â€Ã˜ÂºÃ˜Â´
pcall(function()
	local mt = getrawmetatable(game)
	setreadonly(mt, false)
	local old = mt.__namecall
	mt.__namecall = newcclosure(function(self, ...)
		local args = {...}
		local method = getnamecallmethod()
		if tostring(self) == "Kick" or tostring(method):lower() == "kick" then
			warn("Ã°Å¸Å¡Â« Ã™â€¦Ã˜Â­Ã˜Â§Ã™Ë†Ã™â€Ã˜Â© Ã˜Â·Ã˜Â±Ã˜Â¯ Ã˜ÂªÃ™â€¦ Ã™â€¦Ã™â€ Ã˜Â¹Ã™â€¡Ã˜Â§.")
			return nil
		end
		return old(self, unpack(args))
	end)
end)

-- Ã˜Â¥Ã™â€ Ã˜Â´Ã˜Â§Ã˜Â¡ GUI
local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
gui.Name = "SkyFloatUI"
