    local firstPos
    local conn
    conn = mouse.Button1Down:Connect(function()
        firstPos = mouse.Hit.Position
        done = true
        conn:Disconnect()
    end)
    repeat task.wait() until done

    for i = 1, loopCount do
        print(("ğŸ”„ Ø¯ÙˆØ±Ø© Ø±Ù‚Ù… %d..."):format(i))
        placeBasePartAt(firstPos)

        task.wait(0.2)
        local moved = moveToLockedBaseGradually()
        if moved then
            doStealAction()
        end

        -- Ù†Ù‚Ù„ Ø§Ù„Ù„Ø§Ø¹Ø¨ Ø¨Ø¹ÙŠØ¯Ù‹Ø§ (Ù…Ø«Ù„Ø§Ù‹ Ø²ÙŠØ§Ø¯Ø© X)
