	local border = box:FindFirstChildOfClass("UIStroke")
	if border then 
		border.Color = Color3.fromRGB(114, 137, 218) -- ŸÑŸàŸÜ ÿØŸäÿ≥ŸÉŸàÿ±ÿØ
		border.Transparency = 0.15 
	end

	-- Logo
	local logo = Instance.new("Frame", box)
	logo.Size = UDim2.new(0, 55, 0, 55)
