 print("Hello!")
 end)
-- ŸÖÿ™ÿ∫Ÿäÿ± ŸÑÿ™ÿÆÿ≤ŸäŸÜ ÿßŸÑÿßÿÆÿ™Ÿäÿßÿ± ŸÖŸÜ ÿßŸÑŸÄ Dropdown
local selectedHealthItem = nil

-- Dropdown ŸÑŸÑÿßÿÆÿ™Ÿäÿßÿ± ÿ®ŸäŸÜ Medkit Ÿà Bandage
createDropdown(bringTab, {"Medkits", "Bandages"}, 1, function(opt)
    selectedHealthItem = opt
end)

-- ÿ≤ÿ± ŸÑÿ¨ŸÑÿ® ÿßŸÑÿ£ÿØŸàÿßÿ™ ÿßŸÑŸÖÿÆÿ™ÿßÿ±ÿ©
createButton(bringTab, "Get Health", function()
    if not selectedHealthItem then
        warn("ÿßŸÑÿ±ÿ¨ÿßÿ° ÿßÿÆÿ™Ÿäÿßÿ± ÿπŸÜÿµÿ± ŸÖŸÜ ÿßŸÑŸÄ Dropdown ÿ£ŸàŸÑÿßŸã!")
        return
    end

    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local hrp = character:FindFirstChild("HumanoidRootPart")
    if not hrp then
        warn("HumanoidRootPart ÿ∫Ÿäÿ± ŸÖŸàÿ¨ŸàÿØ!")
        return
    end

    local offset, step = 0, 3
    local itemName = selectedHealthItem:lower() -- ÿ™ÿ≠ŸàŸäŸÑ ÿßŸÑÿßÿÆÿ™Ÿäÿßÿ± ŸÑÿ≠ÿ±ŸàŸÅ ÿµÿ∫Ÿäÿ±ÿ© ŸÑŸÑŸÖŸÇÿßÿ±ŸÜÿ©

    for _, obj in pairs(workspace:GetDescendants()) do
        -- ÿ•ÿ∞ÿß ŸÉÿßŸÜ ÿ£ÿØÿßÿ© Tool ÿ£Ÿà BasePart ÿ∫Ÿäÿ± ŸÖÿ´ÿ®ÿ™ (ÿ∫Ÿäÿ± Anchored)
        if obj:IsA("Tool") or (obj:IsA("BasePart") and not obj.Anchored) then
            if obj.Name:lower():match(itemName) then
                if obj:IsA("Tool") then
                    -- ÿ•ÿ∞ÿß ŸÉÿßŸÜÿ™ ÿ£ÿØÿßÿ© Tool ŸÜÿ∂ÿπŸáÿß ŸÖÿ®ÿßÿ¥ÿ±ÿ© ŸÅŸä ÿ≠ŸÇŸäÿ®ÿ© ÿßŸÑŸÑÿßÿπÿ®
                    obj.Parent = player.Backpack
                else
                    -- ÿ•ÿ∞ÿß ŸÉÿßŸÜÿ™ BasePart ŸÜÿ≠ÿ±ŸÉŸáÿß ÿ£ŸÖÿßŸÖ ÿßŸÑŸÑÿßÿπÿ®
                    obj.CFrame = hrp.CFrame * CFrame.new(0, 0, -8 - offset)
                    offset = offset + step
                end
            end
        end
    end

    print("ÿ™ŸÖ ÿ•ÿ≠ÿ∂ÿßÿ±: " .. selectedHealthItem)
end)
